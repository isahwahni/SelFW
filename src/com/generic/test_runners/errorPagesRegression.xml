<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="errorPagesRegression" parallel="tests" thread-count="10" data-provider-thread-count="5">
	<test name="errorPages firefox">
		<parameter name="browserName" value="firefox"></parameter>
		<classes>
             	<class name="com.generic.tests.errorPages.ErrorPagesValidation" /> 
		</classes>
	</test><!-- Test -->
	<test name="errorPages chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
      	<class name="com.generic.tests.errorPages.ErrorPagesValidation" /> 
		</classes>
	</test><!-- Test -->
	<test name="errorPages mobile">
		<parameter name="browserName" value="mobile_Nexus 5"></parameter>
		<classes>
      	<class name="com.generic.tests.errorPages.ErrorPagesValidation" /> 
		</classes>
	</test><!-- Test -->
</suite> <!-- Suite -->