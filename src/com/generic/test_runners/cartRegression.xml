<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CartRegression" parallel="tests" thread-count="10" data-provider-thread-count="2">
	<test name="cart Tests _ chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="com.generic.tests.Cart.Base_cart" />
		</classes>
	</test><!-- Test -->

	<test name="cart Tests_ firefox">
		<parameter name="browserName" value="firefox"></parameter>
		<classes>
			<class name="com.generic.tests.Cart.Base_cart" />
		</classes>
	</test><!-- Test -->

	<test name="cart Tests_ nexus5">
		<parameter name="browserName" value="mobile_Nexus 5"></parameter>
		<classes>
			<class name="com.generic.tests.Cart.Base_cart" />
		</classes>
	</test><!-- Test -->
</suite> <!-- Suite -->