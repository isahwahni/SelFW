<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="storeLocatorRegression" parallel="tests" thread-count="10" data-provider-thread-count="5">
	<test name="Storelocator chrome">
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="com.generic.tests.storeLocator.StoreLocatorBase" />
		</classes>
	</test><!-- Test -->
	<test name="Storelocator firefox">
		<parameter name="browserName" value="firefox"></parameter>
		<classes>
			<class name="com.generic.tests.storeLocator.StoreLocatorBase" />
		</classes>
	</test><!-- Test -->
	<test name="Storelocator mobile_Nexus 5">
		<parameter name="browserName" value="mobile_Nexus 5"></parameter>
		<classes>
			<class name="com.generic.tests.storeLocator.StoreLocatorBase" />
		</classes>
	</test><!-- Test -->	
</suite> <!-- Suite -->